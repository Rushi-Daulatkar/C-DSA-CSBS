#include <stdio.h>

int main()
{
    int a[10];
    int b[10];
    int i,size;
    
    printf("Enter the Size of array :\n");
    scanf("%d",&size);
    
    printf("Enter the Elements for array A:\n");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    
    printf("Entered the Elements for array A are:\n");
    for(i=0;i<size;i++)
    {
        printf("%d \t",a[i]);
    }

    for(i=0;i<size;i++)
    {
        b[i]=a[i]*a[i];
    }
    
    printf("\nSquare of  Elements for array A are:\n");
    for(i=0;i<size;i++)
    {
        printf("%d \n",b[i]);
    }
}
